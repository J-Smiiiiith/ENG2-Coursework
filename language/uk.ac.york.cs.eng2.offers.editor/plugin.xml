<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated offers -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="offers.presentation.OffersModelWizardID"
            name="%_UI_OffersModelWizard_label"
            class="offers.presentation.OffersModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/OffersModelFile.gif">
         <description>%_UI_OffersModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated offers -->
      <editor
            id="offers.presentation.OffersEditorID"
            name="%_UI_OffersEditor_label"
            icon="icons/full/obj16/OffersModelFile.gif"
            extensions="offers"
            class="offers.presentation.OffersEditor"
            contributorClass="offers.presentation.OffersActionBarContributor">
      </editor>
   </extension>
</plugin>
